{
  "name": "vue2-bootstrap-table2",
  "version": "1.2.1",
  "description": "A sortable and searchable table, as a Vue component, using Bootstrap and Fontawesome styling.",
  "keywords": [
    "table",
    "vuejs",
    "sort",
    "sortable",
    "multicolumn",
    "search",
    "searchable",
    "filter",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jbaysolutions/vue2-bootstrap-table.git"
  },
  "author": "Gustavo Santos (JBay Solutions) <gustavo.santos@jbaysolutions.com> (http://www.jbaysolutions.com)",
  "main": "dist/vue2-bootstrap-table2.common.js",
  "unpkg": "dist/vue2-bootstrap-table2.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "rimraf dist && vue-cli-service build --target lib ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "qs": "^6.0.0",
    "lodash.orderby": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "lodash.includes": "^4.3.0"
  },
  "devDependencies": {
    "es6-promise-promise" : "1.0.0",
    "bootstrap": "^4.3.1",
    "popper.js": "^1.14.7",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "jquery": "^3.4.1",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "rimraf": "^2.6.3",
    "vue-template-compiler": "^2.5.21",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
